<VBox minHeight="400.0" minWidth="800.0" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainViewController">
  <children>
    <MenuBar prefHeight="0.0" prefWidth="922.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="menuFileNew" mnemonicParsing="false" text="New" />
            <MenuItem fx:id="menuFileOpen" mnemonicParsing="false" text="Open…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuFileClose" mnemonicParsing="false" text="Close" />
            <MenuItem fx:id="menuFileSave" mnemonicParsing="false" text="Save" />
            <MenuItem fx:id="menuFileSaveAs" mnemonicParsing="false" text="Save As…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuFileQuit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem fx:id="menuEditUndo" mnemonicParsing="false" text="Undo" />
            <MenuItem fx:id="menuEditRedo" mnemonicParsing="false" text="Redo" />
          </items>
        </Menu>
        <Menu fx:id="menuAbout" mnemonicParsing="false" text="About" />
      </menus>
    </MenuBar>
    <SplitPane dividerPositions="0.0426, 0.2179, 0.7421" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
            <Pane maxWidth="50.0" minWidth="50.0" prefWidth="50.0" style="-fx-background-color: rgb(48,79,254);">
               <children>
                  <ImageView fx:id="btnGamma" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#btnGammaClicked" onMouseEntered="#btnGammaStartHover" onMouseExited="#btnGammaEndHover" opacity="0.5" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ico/gamma_correction.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="btnContrast" fitHeight="50.0" fitWidth="50.0" layoutY="50.0" onMouseClicked="#btnContrastClicked" onMouseEntered="#btnContrastStartHover" onMouseExited="#btnContrastEndHover" opacity="0.5" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../ico/contrast_stretching.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
        <AnchorPane fx:id="pneToolPane">
          <children>
            <Label fx:id="lblCurrentToolName" layoutX="14.0" layoutY="14.0" style="&#10;" text="Tool" textFill="#304ffe" wrapText="false">
              <font>
                <Font size="18.0" fx:id="x1" />
              </font>
            </Label>
          </children>
        </AnchorPane>
        <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
          <content>
            <AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="430.0" />
          </content>
        </ScrollPane>
        <AnchorPane maxWidth="300.0" minWidth="300.0" prefWidth="300.0" SplitPane.resizableWithParent="false">
          <children>
                  <Label fx:id="lblHistogram" layoutX="14.0" layoutY="14.0" style="&#10;" text="Histogram" textFill="#304ffe" wrapText="false">
                     <font>
                        <Font size="18.0" fx:id="x11" />
                     </font>
                  </Label>
                  <AreaChart fx:id="chtHistogram" alternativeRowFillVisible="false" createSymbols="false" horizontalGridLinesVisible="false" horizontalZeroLineVisible="false" layoutX="6.0" layoutY="54.0" legendVisible="false" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" verticalGridLinesVisible="false" verticalZeroLineVisible="false">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis minorTickVisible="false" side="LEFT" tickLabelsVisible="false" tickMarkVisible="false" />
                    </yAxis>
                  </AreaChart>
          </children>
        </AnchorPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
